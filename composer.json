{
  "name": "sanchous98/doctrine-interface-repositories",
  "type": "library",
  "description": "You can create doctrine repositories using interfaces, if the only reason of repository is executing simple queries",
  "authors": [
    {
      "name": "Alexandru Punga",
      "email": "alexandru.punga@atomate.net"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1 || ^8.0",
    "doctrine/orm": "^2.0",
    "symfony/cache": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "InterfaceRepository\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.9"
  },
  "scripts": {
    "static-analysis": "vendor/bin/psalm",
    "test": "vendor/bin/phpunit"
  }
}
